{
  "name": "melis-platform",
  "displayName": "Melis Platform",
  "description": "melis platform",
  "publisher": "simosrennou",
  "version": "0.0.7",
  "engines": {
    "vscode": "^1.85.0"
  },
  "repository": "https://github.com/srennou/melis-snippets",
  "keywords": [
    "Melis",
    "Melis technologies",
    "Melis Plactform",
    "code snippets"
  ],
  "icon": "melis.png",
  "main": "extension.js",
  "activationEvents": [
    "onCommand:melis.addController",
    "onCommand:melis.addTable",
    "onCommand:melis.addService",
    "onCommand:melis.start"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "php",
        "path": "./snippets/php.code-snippets"
      }
    ],
    "commands": [
      {
        "command": "melis.addController",
        "title": "Melis: Add Controller"
      },
      {
        "command": "melis.addTable",
        "title": "Melis: Add Table class"
      },
      {
        "command": "melis.addService",
        "title": "Melis: Add Service"
      },
      {
        "command": "melis.start",
        "title": "Melis DB config"
      }
    ]
  },
  "dependencies": {
    "php-parser": "^3.1.5"
  }
}
